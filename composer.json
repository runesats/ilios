{
    "name": "ilios/ilios",
    "license": "MIT",
    "type": "project",
    "description": "The \"Ilios Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "Ilios\\": "src/Ilios"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">= 7.0",
        "ext-apcu": "*",
        "symfony/symfony": "^3.3.4",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "twig/extensions": "^1.5",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "firebase/php-jwt": "@stable",
        "psr/log": "1.0.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "dreamscapes/ldap-core": "^3.1",
        "eluceo/ical": "0.12.1",
        "exercise/htmlpurifier-bundle": "@stable",
        "ocramius/proxy-manager": "^2.0.0",
        "easycorp/easy-log-handler": "^1.0",
        "happyr/google-analytics-bundle": "^4.0",
        "php-http/httplug-bundle": "^1.3",
        "activelamp/swagger-ui-bundle": "dev-master",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "@stable",
        "symfony/phpunit-bridge": "@stable",
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "matthiasnoback/symfony-dependency-injection-test": "@stable",
        "matthiasnoback/symfony-config-test": "@stable",
        "mockery/mockery": "@stable",
        "fzaninotto/faker": "@stable",
        "liip/functional-test-bundle": "@stable",
        "polishsymfonycommunity/symfony-mocker-container": "@stable"
    },
    "scripts": {
      "symfony-scripts": [
        "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
      ],
      "post-install-cmd": [
        "@symfony-scripts"
      ],
      "post-update-cmd": [
        "@symfony-scripts"
      ]
    },
    "config": {
      "platform": {
        "php": "7.0"
      },
      "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-cache-warmup": true,
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_driver": "ILIOS_DATABASE_DRIVER",
                "database_host": "ILIOS_DATABASE_HOST",
                "database_port": "ILIOS_DATABASE_PORT",
                "database_name": "ILIOS_DATABASE_NAME",
                "database_user": "ILIOS_DATABASE_USER",
                "database_password": "ILIOS_DATABASE_PASSWORD",
                "database_mysql_version": "ILIOS_DATABASE_MYSQL_VERSION",
                "mailer_transport": "ILIOS_MAILER_TRANSPORT",
                "mailer_host": "ILIOS_MAILER_HOST",
                "mailer_user": "ILIOS_MAILER_USER",
                "mailer_password": "ILIOS_MAILER_PASSWORD",
                "locale": "ILIOS_LOCALE",
                "secret": "ILIOS_SECRET",
                "authentication_type": "ILIOS_AUTHENTICATION_TYPE",
                "legacy_password_salt": "ILIOS_LEGACY_PASSWORD_SALT",
                "file_system_storage_path": "ILIOS_FILE_SYSTEM_STORAGE_PATH",
                "institution_domain": "ILIOS_INSTITUTION_DOMAIN",
                "supporting_link": "ILIOS_SUPPORTING_LINK",
                "ldap_authentication_host": "ILIOS_LDAP_AUTHENTICATION_HOST",
                "ldap_authentication_port": "ILIOS_LDAP_AUTHENTICATION_PORT",
                "ldap_authentication_bind_template": "ILIOS_LDAP_AUTHENTICATION_BIND_TEMPLATE",
                "ldap_directory_url": "ILIOS_LDAP_DIRECTORY_URL",
                "ldap_directory_user": "ILIOS_LDAP_DIRECTORY_USER",
                "ldap_directory_password": "ILIOS_LDAP_DIRECTORY_PASSWORD",
                "ldap_directory_search_base": "ILIOS_LDAP_DIRECTORY_SEARCH_BASE",
                "ldap_directory_campus_id_property": "ILIOS_LDAP_DIRECTORY_CAMPUS_ID_PROPERTY",
                "ldap_directory_username_property": "ILIOS_LDAP_DIRECTORY_USERNAME_PROPERTY",
                "shibboleth_authentication_login_path": "ILIOS_SHIBBOLETH_AUTHENTICATION_LOGIN_PATH",
                "shibboleth_authentication_logout_path": "ILIOS_SHIBBOLETH_AUTHENTICATION_LOGOUT_PATH",
                "shibboleth_authentication_user_id_attribute": "ILIOS_SHIBBOLETH_AUTHENTICATION_USER_ID_ATTRIBUTE",
                "timezone": "ILIOS_TIMEZONE",
                "forceProtocol": "ILIOS_FORCEPROTOCOL",
                "keep_frontend_updated": "ILIOS_KEEP_FRONTEND_UPDATED",
                "cas_authentication_server": "ILIOS_CAS_AUTHENTICATION_SERVER",
                "cas_authentication_version": "ILIOS_CAS_AUTHENTICATION_VERSION",
                "cas_authentication_verify_ssl": "ILIOS_CAS_AUTHENTICATION_VERIFY_SSL",
                "cas_authentication_certificate_path": "ILIOS_CAS_AUTHENTICATION_CERTIFICATE_PATH",
                "enable_tracking": "ILIOS_ENABLE_TRACKING",
                "tracking_code": "ILIOS_TRACKING_CODE"
            }
        }
    }
}
